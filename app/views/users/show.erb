<div class="user-info">
  <div class="session-with-avatar">
    <div class="avatar">
      <%= image_tag "test_avatar.jpg" %>
    </div>
    <div class="information">
      <p class="name">Funky super user <span>GMT-5:00 Eastern Time (US & Canada)</span>&nbsp<a class='edit-profile' href="#">edit</a></p>
      <p class="brief-information">I'm humble bundle ruby on rails developer who loves meeting new people</p>
      <p class="bottom-info-label"><span class="highlighted-text">skills: </span>&nbsp node.js, ruby, ruby on rails</p>
    </div>
  </div>
</div>

<div class="user-sessions" ng-app="letspair" ng-controller="PairsessionsUserCtrl">
  <div class="calendar-container">
    <calendar daychange='loadDaySessions()'></calendar>

    <%= link_to 'edit', user_pairsessions_path(current_user.username), class: 'edit-sessions' %>
  </div>

  <ul class="sessions">
    <li class="session-with-contact" ng-repeat="session in sessions">
      
      <pairsessionuser session='session'></pairsessionuser>
      <div class="separator-white"></div>

    </li>
  </ul>
</div>