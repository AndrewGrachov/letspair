<p class="page-title">
  Pair Sessions
</p>

<div class="users-sessions" ng-app="letspair" ng-controller="PairsessionsAllCtrl">
  <div class="calendar-container">
    <calendar daychange='loadDaySessions()'></calendar>
    <div class="cloud">Select a date and see people with open pair programming sessions</div>
  </div>

  <ul class="sessions">
    <div class="no-session" ng-show="sessions.length == 0">no sessions</div>
    <li class="session-full" ng-repeat="session in sessions">
      <pairsessionall opensContactForm session='session'></pairsessionall>
      <div class="separator-white"></div>
    </li>
</div>